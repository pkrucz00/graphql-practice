{
  "name": "graphql-practice",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "build": "yarn prisma generate",
    "dev": "yarn tsx src/index.ts",
    "lint": "yarn eslint . --fix && yarn run prettier --write .",
    "test": "yarn vitest",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.2",
    "prisma": "^6.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.6"
  },
  "dependencies": {
    "@pothos/core": "^4.4.0",
    "@pothos/plugin-prisma": "^4.4.0",
    "@prisma/client": "6.4.1",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.1",
    "graphql-yoga": "^5.12.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run lint"
    ]
  }
}
